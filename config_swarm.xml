<?xml version="1.0" encoding="utf-8"?>
<!-- This is list of customers -->
<swarm>
  <components>
    <comp>
      <name>AND1</name>
      <input>x1,x2</input>
      <output>y</output>
      <function>1-sigmoid(x1*x2,theta,k)</function>
    </comp>
    <comp>
      <name>ANDN</name>
      <input>x1,x2</input>
      <output>y</output>
      <function>sigmoid(x1*(1-x2),theta,k)</function>
    </comp>
    <comp>
      <name>NAND</name>
      <input>x1,x2</input>
      <output>y</output>
      <function>sigmoid(x1*x2,theta,k)</function>
    </comp>
    <comp>
      <name>Negative</name>
      <input>x</input>
      <output>y</output>
      <function>1-sigmoid(x,theta,k)</function>
    </comp>
    <comp>
      <name>NOR1</name>
      <input>x1,x2</input>
      <output>y</output>
      <function>1-sigmoid(x1+x2,theta,k)</function>
    </comp>
    <comp>
      <name>ORN1</name>
      <input>x1,x2</input>
      <output>y</output>
      <function>sigmoid((1-x1)+x2,theta,k)</function>
    </comp>
    <comp>
      <name>Positive</name>
      <input>x</input>
      <output>y</output>
      <function>sigmoid(x,theta,k)</function>
    </comp>
    <comp>
      <name>XNOR</name>
      <input>x1,x2</input>
      <output>y</output>
      <function>1-sigmoid(x1*(1-x2),theta,k)-sigmoid((1-x1)*x2,theta,k)</function>
    </comp>
    <comp>
      <name>XOR1</name>
      <input>x1,x2</input>
      <output>y</output>
      <function>1-sigmoid(x1*(1-x2),theta,k)-sigmoid((1-x1)*x2,theta,k)</function>
    </comp>
  </components>
  <scene>
    <scene_type>通道障碍</scene_type>
    <scene_width>10</scene_width>
    <scene_router>直线</scene_router>
  </scene>
  <problem_setting>
    <problem>围捕</problem>
  </problem_setting>
  <algorithm_setting>
    <algorithm>C-NSGA-II-GP</algorithm>
    <pop_struct_size>20</pop_struct_size>
    <pop_weight_size>10</pop_weight_size>
    <max_iter>100</max_iter>
    <struct_mutation_rate>1.0</struct_mutation_rate>
    <struct_cross_rate>0.1</struct_cross_rate>
    <max_depth>2</max_depth>
    <min_depth>1</min_depth>
    <robot_num>20</robot_num>
  </algorithm_setting>
</swarm>
